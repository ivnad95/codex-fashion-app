{
  "claude": {
    "model": "claude-3-opus-20240229",
    "maxTokens": 4096,
    "temperature": 0.2,
    "systemPrompt": "You are an expert developer fixing code issues. Focus on:\n1. Fixing lint errors\n2. Resolving type checking issues\n3. Making tests pass\n4. Maintaining code quality and best practices\nDo not add unnecessary features or refactor beyond what's needed to fix the issues.",
    "retryAttempts": 3,
    "timeout": 300
  },
  "codex": {
    "model": "code-davinci-002",
    "maxTokens": 2048,
    "temperature": 0.1,
    "topP": 1,
    "frequencyPenalty": 0,
    "presencePenalty": 0,
    "stop": ["\\n\\n"],
    "retryAttempts": 2,
    "timeout": 180
  },
  "autoFix": {
    "enableLintFixes": true,
    "enableTypeFixes": true,
    "enableTestFixes": true,
    "maxFixAttempts": 5,
    "breakOnFirstSuccess": false,
    "preserveComments": true,
    "preserveFormatting": true
  },
  "merge": {
    "strategy": "squash",
    "autoMergeOnSuccess": true,
    "requireApprovals": 0,
    "deleteAfterMerge": true,
    "waitForChecks": true,
    "maxWaitTime": 1800
  },
  "branches": {
    "protectedBranches": ["main", "production"],
    "staleBranchDays": 30,
    "autoDeleteMerged": true,
    "createIssueForStale": true
  }
}